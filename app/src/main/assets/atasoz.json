[
  {
    "text": "Abanın kadri yağmurda bilinir.",
    "author": "Anonim"
  },
  {
    "text": "Aç tavuk kendini darı ambarında zanneder.",
    "author": "Anonim"
  },
  {
    "text": "Acele bir ağaçtır, meyvesi pişmanlık.",
    "author": "Anonim"
  },
  {
    "text": "Akarsu çukurunu kendi kazar.",
    "author": "Anonim"
  },
  {
    "text": "Akıl akıldan üstündür.",
    "author": "Anonim"
  },
  {
    "text": "Akıl kişiye/adama/yiğide sermayedir.",
    "author": "Anonim"
  },
  {
    "text": "Akıl para ile satılmaz.",
    "author": "Anonim"
  },
  {
    "text": "Akıl yaşta değil, baştadır.",
    "author": "Anonim"
  },
  {
    "text": "Aklı başında olan kuru kavgaya düşmez.",
    "author": "Anonim"
  },
  {
    "text": "Allah gümüş kapıyı kaparsa altın kapıyı açar.",
    "author": "Anonim"
  },
  {
    "text": "Allah’tan umut kesilmez.",
    "author": "Anonim"
  },
  {
    "text": "Altın yumurtlayan tavuk kesilmez.",
    "author": "Anonim"
  },
  {
    "text": "Ava giden avlanır.",
    "author": "Anonim"
  },
  {
    "text": "Az veren candan, çok veren maldan.",
    "author": "Anonim"
  },
  {
    "text": "Azimle yüce dağlar devrilir.",
    "author": "Anonim"
  },
  {
    "text": "Azmin elinden ne uçan kurtulur ne kaçan.",
    "author": "Anonim"
  },
  {
    "text": "Bugün bana ise yarın sana.",
    "author": "Anonim"
  },
  {
    "text": "Burnu büyük olanın aklı küçük olur.",
    "author": "Anonim"
  },
  {
    "text": "Büyük lokma ye, büyük söz söyleme.",
    "author": "Anonim"
  },
  {
    "text": "Çam sakızı, çoban armağanı.",
    "author": "Anonim"
  },
  {
    "text": "Çanağa ne doğrarsan kaşığında o çıkar.",
    "author": "Anonim"
  },
  {
    "text": "Cennetin kapısını cömertler açar.",
    "author": "Anonim"
  },
  {
    "text": "Cimrinin zararı, cömertin kârından fazla olur.",
    "author": "Anonim"
  },
  {
    "text": "Deveden büyük fil var.",
    "author": "Anonim"
  },
  {
    "text": "Dilim seni dilim dilim dileyim, başıma geleni senden bileyim.",
    "author": "Anonim"
  },
  {
    "text": "Doğru söz yemin istemez.",
    "author": "Anonim"
  },
  {
    "text": "Dost dostun eğerlenmiş atıdır.",
    "author": "Anonim"
  },
  {
    "text": "Dost kara günde belli olur.",
    "author": "Anonim"
  },
  {
    "text": "El elden üstündür.",
    "author": "Anonim"
  },
  {
    "text": "Eski dost düşman olmaz, yenisinden vefa gelmez.",
    "author": "Anonim"
  },
  {
    "text": "Gelen geçer, konan göçer.",
    "author": "Anonim"
  },
  {
    "text": "Gönül kimi severse güzel odur.",
    "author": "Anonim"
  },
  {
    "text": "Güzele kırk günde doyulur, iyi huyluya kırk yılda doyulmaz.",
    "author": "Anonim"
  },
  {
    "text": "Hak yerini bulur.",
    "author": "Anonim"
  },
  {
    "text": "Her güzelin bir kusuru vardır.",
    "author": "Anonim"
  },
  {
    "text": "Her işin başı sağlık.",
    "author": "Anonim"
  },
  {
    "text": "İşleyen demir pas tutmaz.",
    "author": "Anonim"
  },
  {
    "text": "İyiliğe iyilik her kişinin kârı, kötülüğe iyilik er kişinin kârı.",
    "author": "Anonim"
  },
  {
    "text": "İyilik eden iyilik bulur.",
    "author": "Anonim"
  },
  {
    "text": "İyilik et denize at, balık bilmezse Halik (Allah) bilir.",
    "author": "Anonim"
  },
  {
    "text": "Kazma elin kuyusunu, kazarlar kuyunu.",
    "author": "Anonim"
  },
  {
    "text": "Kul sıkılmayınca hızır yetişmez.",
    "author": "Anonim"
  },
  {
    "text": "Merhametten maraz doğar.",
    "author": "Anonim"
  },
  {
    "text": "Misafir kısmeti ile gelir.",
    "author": "Anonim"
  },
  {
    "text": "Misafirlik üç gündür.",
    "author": "Anonim"
  },
  {
    "text": "Ne ekersen onu biçersin.",
    "author": "Anonim"
  },
  {
    "text": "Ne oldum dememeli, ne olacağım demeli.",
    "author": "Anonim"
  },
  {
    "text": "Ne verirsen elinle, o gider seninle.",
    "author": "Anonim"
  },
  {
    "text": "Öfkeyle kalkan zararla oturur.",
    "author": "Anonim"
  },
  {
    "text": "Hastalık sağlık bizim (insan) içindir.",
    "author": "Anonim"
  },
  {
    "text": "Say beni sayayım seni.",
    "author": "Anonim"
  },
  {
    "text": "Şefkati olmayan şefkat bulamaz.",
    "author": "Anonim"
  },
  {
    "text": "Son pişmanlık fayda etmez.",
    "author": "Anonim"
  },
  {
    "text": "Tok, açın halinden anlamaz.",
    "author": "Anonim"
  },
  {
    "text": "Veren el, alandan üstündür.",
    "author": "Anonim"
  },
  {
    "text": "Veren eli herkes öper.",
    "author": "Anonim"
  },
  {
    "text": "Yapılan iyilik asla unutulmaz.",
    "author": "Anonim"
  },
  {
    "text": "Zenginin gönlü olana kadar fakirin canı çıkar.",
    "author": "Anonim"
  },
  {
    "text": "Zor oyunu bozar.",
    "author": "Anonim"
  }
]
